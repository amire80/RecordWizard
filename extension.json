				"mwe-recwiz-studio-uploadcount",
                "mwe-recwiz-pendinguplads",
                "mwe-recwiz-allsucceeded",
                "mwe-recwiz-somefailed",
                "mwe-recwiz-allfailed",
                "mwe-recwiz-error-noname",
				"mwe-recwiz-error-duplicatename",
				"mwe-recwiz-error-nolanguages",
				"mwe-recwiz-error-emptylist",
                "mwe-recwiz-warning-wordsleft",
                "mwe-recwiz-warning-pendinguploads",
                "mwe-recwiz-warning-faileduploads"
			]
        },
        "ext.recordWizard.wikibase": {
            "scripts": [
                "modules/wikibase/mw.recordWizard.wikibase.js",
                "modules/wikibase/mw.recordWizard.wikibase.Snak.js",
                "modules/wikibase/mw.recordWizard.wikibase.Reference.js",
                "modules/wikibase/mw.recordWizard.wikibase.Statement.js",
                "modules/wikibase/mw.recordWizard.wikibase.Item.js"
            ],
			"dependencies": [
			    "wikibase.datamodel",
			    "wikibase.serialization"
			]
        },
        "ext.recordWizard.controller": {
            "scripts": [
                "modules/controller/mw.recordWizard.controller.js",
                "modules/controller/mw.recordWizard.controller.Step.js",
                "modules/controller/mw.recordWizard.controller.Tutorial.js",
                "modules/controller/mw.recordWizard.controller.Locutor.js",
                "modules/controller/mw.recordWizard.controller.Details.js",
                "modules/controller/mw.recordWizard.controller.Studio.js",
                "modules/controller/mw.recordWizard.controller.Publish.js"
            ],
