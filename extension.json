                "mwe-recwiz-gender-female",
                "mwe-recwiz-gender-other",
                "mwe-recwiz-details-lang",
                "mwe-recwiz-details-wordlist",
                "mwe-recwiz-details-randomise",
                "mwe-recwiz-details-addwords",
                "mwe-recwiz-details-clear",
                "mwe-recwiz-details-noword",
                "mwe-recwiz-details-wordcount",
