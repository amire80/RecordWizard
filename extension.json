                "modules/layout/mw.recordWizard.layout.LicenseSelectorWidget.js",
                "modules/layout/mw.recordWizard.layout.WordSelectorWidget.js",
                "modules/layout/mw.recordWizard.layout.WikidataSearchWidget.js"
