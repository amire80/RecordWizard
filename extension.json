			    "oojs-ui",
			    "mediawiki.ForeignApi",
			    "mediawiki.widgets",
			    "oojs-ui.styles.icons-location"
			],
			"messages": [
                "mwe-recwiz-generator-nearby",
                "mwe-recwiz-generator-list",
			    "mwe-recwiz-nearby-coordinates",
			    "mwe-recwiz-nearby-coordinates-help",
			    "mwe-recwiz-nearby-limit",
